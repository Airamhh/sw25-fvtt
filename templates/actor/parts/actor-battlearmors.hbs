<ol class='items-list'>
  <li class='item flexrow items-header'>
    <div class='item-name'>{{localize "TYPES.Item.armor"}} ({{localize "SW25.Item.Equip"}})</div>
    <div class='item-element' style="flex: 1 0 0;">{{localize "SW25.Item.Category"}}</div>
    <div class='item-element' style="flex: 0 0 30px;">{{localize "SW25.Item.Armor.Dodge"}}</div>
    <div class='item-element' style="flex: 0 0 30px;">{{localize "SW25.Attributes.Protectionpoint.abbr"}}</div>
    <div class='item-element' style="flex: 0 0 30px;">{{localize "SW25.Attributes.Magicprotection.abbr"}}</div>
  </li>
  {{#each battlearmors as |item id|}}
    <li class='item flexrow' data-item-id='{{item._id}}'>
      <div class='item-name'>
        <div class='item-image'>
          <a class='rollable' data-roll-type='item'>
            <img
              src='{{item.img}}'
              title='{{item.name}}'
              width='24'
              height='24'
            />
          </a>
        </div>
        <h4>{{item.name}}{{#if item.system.dedicated}} ({{localize "SW25.Item.Dedicated"}}){{/if}}</h4>
      </div>
      <div class='item-quantity item-prop flexrow flex-group-center' style="flex: 1 0 0;">
        {{item.system.categoryname}}{{item.system.rank}}
      </div>
      <div class='item-quantity item-prop flexrow flex-group-center' style="flex: 0 0 30px;">
        {{item.system.dodge}}
      </div>
      <div class='item-quantity item-prop flexrow flex-group-center' style="flex: 0 0 30px;">
        {{item.system.pp}}
      </div>
      <div class='item-quantity item-prop flexrow flex-group-center' style="flex: 0 0 30px;">
        {{item.system.mpp}}
      </div>
    </li>
  {{/each}}
</ol>